1 szerializálás
  store($adat, $filenev)	# tárolás fájlba
  store_fd($adat, *FH)		# megnyitott fájlkezelőbe (binmode(FH))
  $szerializalt = freeze($adat)	# memóriába
  
  retrieve($filenev)
  fd_retrieve(*FH)
  thaw($szerializált)


